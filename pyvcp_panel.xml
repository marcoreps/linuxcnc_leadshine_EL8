<pyvcp>
	
    <labelframe text="EL2008">
		<button>
			<halpin>"dout0"</halpin>
			<text>"Relay 1"</text>
		</button>
		<button>
			<halpin>"dout1"</halpin>
			<text>"Relay 2"</text>
		</button>
    </labelframe>
    
    <labelframe text="Servos">
		<label>
            <text>"X Axis Load:"</text>
        </label>
        <bar>
            <halpin>"x-load"</halpin>
