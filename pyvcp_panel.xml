        </bar>
        <label>
            <text>"Z Axis Load:"</text>
        </label>
        <bar>
            <halpin>"y-load"</halpin>
            <max_>1000</max_>
        </bar>
        <label>
            <text>"Z Axis Load:"</text>
        </label>
        <bar>
            <halpin>"z-load"</halpin>
            <max_>1000</max_>
        </bar>
    </labelframe>
    
</pyvcp>
